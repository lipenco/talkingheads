<div id="show-conference">
   
    <p><%= @name %></p>
     <p><%= @date %></p>
     <p><%= @place %></p>
      <p><%= @tags %></p>
     <p> <%= @organizers %></p>
     <p><%= @description %></p>

<% if @talks.length: %>
  <% for talks in @talks: %>
    <h2><%= talks.title %></h2>
    <p><%= talks.video_url %></p>
  <% end %>
<% end %>

 </div>