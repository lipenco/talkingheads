<div id="show-conference">
    <legend>Details</legend>
    <p><%= @name %></p>
     <p><%= @date %></p>
     <p><%= @place %></p>
      <p><%= @tags %></p>
     <p> <%= @organizers %></p>
     <p><%= @description %></p>

<% if @talks.length: %>
  <% for talks in @talks: %>
    <a href=""><%= talks.title %></a>
    <p><%= talks.video_url %></p>
  <% end %>
<% end %>

 </div>